<h1>Users</h1>

<table>
    <tr>
        <th>ID</th>
        <th>Email</th>
        <th>Name</th>
        <th>Age</th>
        <th>Delete</th>
    </tr>
    {{#users}}
        <tr>
            <td>{{id}}</td>
            <td>{{email}} JPY</td>
            <td>{{name}}</td>
            <td>{{age}}</td>
            <td><a href="/users/delete/{{id}}">Delete</a></td>
        </tr>
    {{/users}}
</table>